{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Run Current Script",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "run-script"],
      "skipFiles": ["<node_internals>/**"],
      "console": "integratedTerminal",
      "env": {
        "TS_NODE_PROJECT": "./tsconfig.script.json"
      }
    }
  ]
}
